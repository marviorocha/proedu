     <%= content_for :title do resource_name.to_s.humanize end %>

     <div class="row user">
       <div class="col-md-12">
         <div class="profile">
           <div class="info">
           

 
          
 
 
            

           <%#= image_tag(current_user.avatar_user, class: "app-sidebar__user-avatar") if current_user.avatar_user.atteched? %>
       
             <h4><%= full_name %></h4>
             <p><%= user_type %></p>
           </div>
           <div class="cover-image"></div>
         </div>
       </div>
       <div class="col-md-3">
         <div class="tile p-0">
           <ul class="nav flex-column nav-tabs user-tabs">
             <li class="nav-item"><a class="nav-link active" href="#user-timeline" data-toggle="tab">Timeline</a></li>
             <li class="nav-item"><a class="nav-link" href="#user-settings" data-toggle="tab">Editar
                 <%= t('profile') %></a></li>
           </ul>
         </div>
       </div>
       <div class="col-md-9">
         <div class="tab-content">
          <div class="tab-pane active" id="user-timeline">
           <%= render('devise/registrations/tab-content') %> 
           </div>
           <div class="tab-pane fade" id="user-settings">
             <div class="tile user-settings">
               <h4 class="line-head">Editar <%= t("profile") %></h4>
               <%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: { method: :put, multipart: true }) do |f| %>
               <%= render "devise/shared/error_messages", resource: resource %>
               <div class="row mb-4">
                 <div class="col-md-4">
                   <%= f.label :fname, "Nome:" %>
                   <%= f.text_field :fname, class: "form-control", placeholder: "Digite seu nome" %>
                 </div>
                 <div class="col-md-4">
                   <%= f.label :lname, "Sobre nome:" %>
                   <%= f.text_field :lname, class: "form-control", placeholder: "Sobre nome" %>
                 </div>
               </div>
               <div class="row">
                 <div class="col-md-8 mb-4">
                   <%= f.label :email, "Email:" %>
                   <%= f.text_field :email, class: "form-control", autofocus: true, autocomplete: "email", placeholder: "Digite seu e-mail" %>
                   <% if devise_mapping.confirmable? && resource.pending_reconfirmation? %>
                   <div>Atualmente aguardando confirmação por: <%= resource.unconfirmed_email %></div>
                   <% end %>
                 </div>

               <div class="col-md-8 mb-4">
                   <%= f.label :avatar_user, "Adicionar foto:" %>
                   <%= f.file_field :avatar_user, class: "form-control", direct_upload: false %>
                  
                 </div>

                 <div class="clearfix"></div>
                 <div class="col-md-8 mb-4">
                   <%= f.label :birthday, "Data de aniversário:" %>
                   <%= f.text_field :birthday, class: "form-control datepicker" %>

                 </div>
                 <div class="clearfix"></div>
                 <div class="col-md-8 md-4">
                   <%= f.label :password %> <i>(<%= t('password_blank') %>)</i><br />
                   <%= f.password_field :password, autocomplete: "new-password", class: "form-control" %>
                   <% if @minimum_password_length %>
                   <br />
                   <em><%= @minimum_password_length %> characters minimum</em>
                   <% end %>

                 </div>
                 <div class="col-md-8 mb-4">
                   <%= f.label :password_confirmation %><br />
                   <%= f.password_field :password_confirmation, autocomplete: "new-password", class: "form-control"%>
                 </div>
                 <div class="clearfix"></div>
                 <div class="col-md-8 mb-4">
                   <%= f.label :current_password %> <i>(we need your current password to confirm your changes)</i><br />
                   <%= f.password_field :current_password, autocomplete: "current-password", class: "form-control" %>
                 </div>


               </div>
               <div class="row mb-10">
                 <div class="col-md-12">
                   <%= button_tag type: "submit", disable_with: "Atualizando...", class: "btn btn-primary" do  %>
                   <i class="fa fa-fw fa-lg fa-check-circle"></i> Atualizar</button>
                   <% end %>
                 </div>
               </div>
               </form>
             </div>
           </div>
         </div>
       </div>
     </div>

     <% end %>
     <%= link_to "Back", :back %>